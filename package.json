    "typescript": "^5.6.3"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python3 -m uvicorn main:app --reload --port 8000",
    "test:frontend": "cd frontend && npm run test:ci",
    "test:backend": "cd backend && python3 -m pytest",
    "test:all": "npm run test:frontend && npm run test:backend",
    "build:frontend": "cd frontend && npm run build",
    "clean:all": "cd frontend && npm run clean && cd .. && rm -rf .pytest_cache __pycache__ *.log"
  }
}
